#include <iostream>
using namespace std;
int main()
{
	double sales = 95000;
	double stateTax = sales * 0.04;
	double countyTax = sales * 0.02;
	double totalTax = stateTax + countyTax;
	cout << "Sales = $" << sales << endl << "State tax = $" << stateTax << endl << "County tax = $" << countyTax << endl << "Total tax = $" << totalTax;
	return 0;
}

